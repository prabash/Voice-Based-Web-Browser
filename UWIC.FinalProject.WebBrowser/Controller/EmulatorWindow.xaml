<UserControl x:Class="UWIC.FinalProject.WebBrowser.Controller.EmulatorWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             DataContext="{Binding RelativeSource={RelativeSource Self}}" 
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="220">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="5">
            <TextBox Width="140" Text="{Binding EmulatorCommand, Mode=TwoWay}"/>
            <Button Content="Emulate" Margin="5,0,0,0" Command="{Binding FunctionCommand}"/>
        </StackPanel>
    </Grid>
</UserControl>
